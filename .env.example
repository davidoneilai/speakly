OPENAI_API_KEY=sk-...
OPENAI_LLM_MODEL=gpt-4o-mini

# Configurações do Whisper
# Modelo: tiny (mais rápido), base (recomendado), small, medium, large
WHISPER_MODEL=base

# Idioma: auto, pt, en, es, fr, etc.
WHISPER_LANGUAGE=auto

# Configurações de TTS
# Provider: 'auto' (padrão), 'openai', 'gtts'
TTS_PROVIDER=auto

# Qualidade: 'fast', 'normal' (padrão), 'high'
TTS_QUALITY=normal

# Idioma para gTTS: 'en' (padrão), 'es', 'fr', 'pt', 'zh', etc.
TTS_LANG=en